    <webLinks>
        <fullName>Cancel_Deployment</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel Deployment</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
var updateRecords = sforce.connection.query(&quot;select id from {!$Setup.Settings__c.Namespace__c}Deployment_Job__c where {!$Setup.Settings__c.Namespace__c}Step__r.{!$Setup.Settings__c.Namespace__c}Deployment__c = &apos;{!Deployment__c.Id}&apos;and {!$Setup.Settings__c.Namespace__c}Status__c in (&apos;Pending&apos;,&apos;Not started&apos;)&quot;).getArray(&quot;records&quot;);
if(updateRecords == null || updateRecords.length == 0){
	alert(&quot;{!$Label.NOTHING_TO_CANCEL}&quot;);
}
else{
  for (var i = 0; i &lt; updateRecords.length; i++) {
    updateRecords[i].{!$Setup.Settings__c.Namespace__c}Status__c = &quot;Cancelled&quot;;
  }
  var result = sforce.connection.update(updateRecords);
  window.location.reload();
}</url>
    </webLinks>
